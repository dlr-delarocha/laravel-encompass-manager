{
    "name": "delarocha/laravel-encompass-manager",
    "description": "Encompass Manager for Laravel",
    "keywords": ["Encompass", "sdk", "manager"],
    "type": "library",
    "homepage": "https://github.com/dlr-delarocha/php-encompass-manager.git",
    "require": {
        "php":  ">=7.1",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alberto de la Rocha",
            "email": "delarocha.alberto01@gmail.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/dlr-delarocha/laravel-encompass-manager/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Encompass\\": "src/Encompass/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encompass\\ServiceProvider"
            ]
        }
    }
}
